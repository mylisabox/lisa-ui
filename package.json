{
  "name": "lisa-ui",
  "version": "0.0.1",
  "license": "GPL-3.0",
  "angular-cli": {},
  "scripts": {
    "start": "ng serve --host 0.0.0.0",
    "build": "npm run buildEN && npm run buildFR",
    "buildEN": "ng build --output-path=bundle/bundle-en --prod --aot --i18nFile=./src/locale/messages.en.xlf --locale=en --i18nFormat=xlf",
    "buildFR": "ng build --output-path=bundle/bundle-fr --prod --aot --i18nFile=./src/locale/messages.fr.xlf --locale=fr --i18nFormat=xlf",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "i18n": "ng xi18n --output-path=src/locale --locale=en && xliffmerge --profile xliffmerge.json en fr",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^4.3.6",
    "@angular/compiler": "^4.3.6",
    "@angular/core": "^4.3.6",
    "@angular/forms": "^4.3.6",
    "@angular/http": "^4.3.6",
    "@angular/platform-browser": "^4.3.6",
    "@angular/platform-browser-dynamic": "^4.3.6",
    "@angular/router": "^4.3.6",
    "angular2-jwt": "^0.2.3",
    "bootstrap-sass": "^3.3.7",
    "core-js": "^2.4.1",
    "express": "^4.15.3",
    "ngx-bootstrap": "^1.9.1",
    "ngx-color-picker": "^4.3.1",
    "ngx-dashboard": "^1.2.0",
    "rxjs": "^5.4.3",
    "zone.js": "0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.2.3",
    "@angular/compiler-cli": "^4.3.1",
    "@angular/platform-server": "^4.3.1",
    "@types/jasmine": "^2.5.47",
    "@types/node": "^7.0.31",
    "codelyzer": "~2.1.1",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "3.2.0",
    "karma": "1.4.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ngx-i18nsupport": "^0.6.2",
    "protractor": "~5.1.0",
    "ts-node": "~2.0.0",
    "tslint": "~4.5.0",
    "typescript": "~2.4.0"
  }
}
